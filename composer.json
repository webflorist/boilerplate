{
    "name": "webflorist/boilerplate",
    "description": "Boilerplate functionality for a Laravel (5.8+) application.",
    "authors": [
        {
            "name": "Gerald Buttinger",
            "email": "gerald@code.florist"
        }
    ],
    "require": {
        "php": "~7.1",
        "laravel/framework": "~5.8",
        "google/cloud-error-reporting": "^0.14.5",
        "google/cloud-logging": "^1.16",
        "webflorist/htmlfactory": "dev-develop",
        "webflorist/routetree": "2.0-dev",
        "umpirsky/country-list": "^2.0",
        "umpirsky/language-list": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8",
        "gajus/dindent": "^2.0",
        "ext-dom": "*"
    },
    "autoload": {
        "psr-4": {
            "Webflorist\\Boilerplate\\": "src/",
            "BoilerplateTests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/webflorist/routetree",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/webflorist/htmlfactory",
            "no-api": true
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Webflorist\\Boilerplate\\BoilerplateServiceProvider"
            ],
            "aliases": {
                "Boilerplate": "Webflorist\\Boilerplate\\BoilerplateFacade"
            }
        }
    }
}
